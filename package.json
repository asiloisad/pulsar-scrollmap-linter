{
  "name": "scrollmap-linter",
  "description": "Linter message markers for scrollmap",
  "repository": "https://github.com/asiloisad/pulsar-scrollmap-linter",
  "keywords": [
    "scrollmap",
    "linter",
    "error",
    "warning",
    "marker"
  ],
  "version": "0.2.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "providedServices": {
    "scrollmap": {
      "versions": {
        "1.0.0": "provideScrollmap"
      }
    },
    "linter-ui": {
      "versions": {
        "1.0.0": "provideLinterUI"
      }
    }
  },
  "configSchema": {
    "threshold": {
      "order": 1,
      "type": "integer",
      "title": "Threshold",
      "description": "Hide markers if count exceeds this value. Zero means no limit.",
      "minimum": 0,
      "default": 0
    }
  }
}
